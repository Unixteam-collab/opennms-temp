      <uei>uei.opennms.org/traps/JVM-MANAGEMENT-MIB/jvmLowMemoryPoolUsageNotif</uei>
      <event-label>JVM-MANAGEMENT-MIB defined trap event: jvmLowMemoryPoolUsageNotif</event-label>
      <descr>
&lt;p>This notification is sent when the memory usage threshold of
	    a memory pool is exceeded.
           &lt;/p>&lt;table>
	&lt;tr>&lt;td>&lt;b>

	jvmMemPoolName&lt;/b>&lt;/td>&lt;td>
	%parm[#1]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	jvmMemPoolUsed&lt;/b>&lt;/td>&lt;td>
	%parm[#2]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>
	&lt;tr>&lt;td>&lt;b>

	jvmMemPoolThreshdCount&lt;/b>&lt;/td>&lt;td>
	%parm[#3]%;&lt;/td>&lt;td>&lt;p>&lt;/p>&lt;/td>&lt;/tr>&lt;/table>
	</descr>
      <logmsg dest="logndisplay">&lt;p>
	jvmLowMemoryPoolUsageNotif trap received
	jvmMemPoolName=%parm[#1]%
	jvmMemPoolUsed=%parm[#2]%
	jvmMemPoolThreshdCount=%parm[#3]%&lt;/p>
	</logmsg>
      <severity>Indeterminate</severity>
   </event>
